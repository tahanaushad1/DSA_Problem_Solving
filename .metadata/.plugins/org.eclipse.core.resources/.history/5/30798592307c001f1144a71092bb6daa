package com.demo;

public class Sorting {
	public static void printArry(int arr[]) {
		for(int i=0;i<arr.length;i++) {
			System.out.print(arr[i]);
		}
		System.out.println();
	}
	public static void main(String[] args) {
		int arr[]= {9,1,4,6};
		for(int i=0;i<arr.length-1;i++) {
			for(int j=0;i<arr.length-i-1;i++) {
				if(arr[j]>arr[j+1]) {
					int temp=arr[j];
					arr[j]=arr[j+1];
					arr[j+1]=temp;
				}
				
			}
		}
		printArry(arr);
		
	}

}
